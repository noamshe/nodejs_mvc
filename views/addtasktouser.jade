extends layout
block content
    h1= title
    form#formAddUser(name="adduser",method="post",action="/saveuserwithtask")
        select(name="selectUser")
            each user in users
                option(value=user.username) #{user.username}
        input#inputUserName(type="text", placeholder="task", name="task")
        button#btnSubmit(type="submit") submit
